{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "extends": ["eslint:recommended", "google"],
  "rules": {
    "max-len": "off",
    "require-jsdoc": "off",
    "no-extend-native": "off",
    "no-invalid-this": "off",
    "linebreak-style": ["error", "windows"]
  }
}
